#include <stdio.h>
#include <stdlib.h>
#include<math.h>

int main()
{
int n,rem,res,res1,count,t,m,sum=0;
int srem,sres,res2,sum1=0;
printf("enter the number\n");
scanf("%d",&n);
while(n!=0)
{
   rem=n%10;
   res=n-rem;
   res1=res/10;
   n=res1;
   res2=rem+1;
   sum=res2+sum;
}
 printf("\nincreased digits sum:%d",sum);

return;
   }
