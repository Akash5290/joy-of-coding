#include <stdio.h>
#include <stdlib.h>

int main()
{
   int num1,num2;
   printf("enter num1:");
   scanf("%d",&num1);
   printf("enter num2:");
   scanf("%d",&num2);
   if(num1>0&&num2>0)
   {  if(num1>num2)
     {
      printf("the largest is %d\n",num1);
     }
      else if(num2>num1)
     {
       printf("the largest is %d\n",num2);
     }
      else
     {
       printf("%d is the largest\n",num1);
     }
   }
   else
   {
       printf("invalid number in input");
   }
   return;
   }
