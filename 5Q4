#include <stdio.h>
#include<stdlib.h>
int main()
{
int c[10],i,rep[10],count=0;
printf("enter ten elements:\n");
for(int k=0;k<10;k++){
    scanf("%d",&c[k]);
}

for(int i=0;i<10;i++)
{    count=0;
    for(int j=0;j<10;j++)
    {
        if(i==j)
        {
         count++;
        }
        else if(c[i]==c[j])
        {
            count++;
        }
    }
     printf("%d is repeated %d times\n",c[i],count);
}


return 1;
}
