#include <stdio.h>
#include <stdlib.h>

int main()
{
   int num1,num2,num3;
   printf("enter num1\n");
   scanf("%d",&num1);
   printf("enter num2\n");
   scanf("%d",&num2);
   printf("enter num3\n");
   scanf("%d",&num3);
   if(num1>0&&num2>0&&num3>0)
  {

   if (num1>num2&&num1>num3)
   {
      printf("%d is the largest\n",num1);
   }
   else if(num2>num1&&num2>num3)
   {
       printf("%d is the largest\n",num2);
          }
   else
   {
       printf("%d is the largest\n",num3);
   }

  }
  else
  {
      printf("input is invalid\n");
  }

   return;
   }
