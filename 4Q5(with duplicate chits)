#include <stdio.h>
#include <stdlib.h>
#include<conio.h>

int main()
{
int i,a[10];
printf("enter numbers from 1 to 10(U can jumble the numbers):\n");
for(i=0;i<10;i++)
{
    scanf("%d",&a[i]);
}
for(i=0;i<10;i++)
{
    if(a[i]==9)
    {
        printf("number of guesses:%d\n",i);
    }
}
return;
}
