#include <stdio.h>
#include<stdlib.h>
int main()
{
int n=10, c[10]={2 ,6, 10, 14, 18, 3, 7, 11, 15, 19},i,pos,x;
printf("initial array:");
for(i=0;i<10;i++)
{
    printf(" %d ",c[i]);
}
pos=5;
x=22;
n++;
for(i=n-1;i>=pos;i--)
{
    c[i]=c[i-1];
}
c[pos-1]=x;
printf("\nnew updated array");
for(i=0;i<11;i++)
{
    printf("  %d",c[i]);
}
    return 0;
}
